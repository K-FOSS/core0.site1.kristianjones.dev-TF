{
  "api": {
    "enabled": true,
    "bind": "0.0.0.0:8888"
  },
  "metrics": {
    "enabled": true,
    "bind": ":9284"
  },
  "servers": {
    "rns": {
      "bind": "0.0.0.0:53",
      "protocol": "udp",
      "backend_idle_timeout": "5s",
      "udp": {
        "max_requests": 32
      },
      "discovery": {
        "kind": "consul",
        "consul_host": "${Consul.Hostname}:${Consul.Port}",
        "consul_acl_token": "${Consul.Token}",
        "consul_service_name": "dns",
        "consul_service_tag": "dns.rns"
      }
    }
  }
}