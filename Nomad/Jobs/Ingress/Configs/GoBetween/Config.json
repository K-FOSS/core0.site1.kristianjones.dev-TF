{
  "$schema": "",
  "api": {
    "enabled": true,
    "bind": "0.0.0.0:8888"
  },
  "metrics": {
    "enabled": true,
    "bind": ":9284"
  },
  "servers": {
    "unifi-inform": {
      "bind": "0.0.0.0:8080",
      "protocol": "tcp",
      "discovery": {
        "kind": "srv",
        "srv_lookup_server": "172.17.0.10:53",
        "srv_lookup_pattern": "kjdev-unifi-comm.ix-kjdev-unifi.svc.cluster.local."
      }
    },
    "https-tcp": {
      "bind": "0.0.0.0:443",
      "protocol": "tcp",
      "discovery": {
        "kind": "srv",
        "srv_lookup_server": "{{ env "NOMAD_IP_nodehttps" }}:8600",
        "srv_lookup_pattern": "https.web.service.kjdev."
      }
    },
    "https-udp": {
      "bind": "0.0.0.0:443",
      "protocol": "udp",
      "backend_idle_timeout": "5s",
      "udp": {
        "max_requests": 32
      },
      "discovery": {
        "kind": "srv",
        "srv_lookup_server": "{{ env "NOMAD_IP_nodehttps" }}:8600",
        "srv_lookup_pattern": "https.web.service.kjdev."
      }
    },
    "http-tcp": {
      "bind": "0.0.0.0:80",
      "protocol": "tcp",
      "discovery": {
        "kind": "srv",
        "srv_lookup_server": "{{ env "NOMAD_IP_nodehttps" }}:8600",
        "srv_lookup_pattern": "http.web.service.kjdev."
      }
    },
    "http-minio": {
      "bind": "0.0.0.0:9080",
      "protocol": "tcp",
      "discovery": {
        "kind": "srv",
        "srv_lookup_server": "{{ env "NOMAD_IP_nodehttps" }}:8600",
        "srv_lookup_pattern": "http.minio.web.service.kjdev."
      }
    }
  }
}